include ./includes/gemeente.pug
        .container.theme-showcase(role='main')
            // Main jumbotron for a primary marketing message or call to action
            .page-header
                h1
                    | SocialCoin | Wallet '#{address}'
                    .pull-right
                        button.btn.btn-default(onclick='goBack()') Terug
            .row
                .col-md-6
                    h3 #{title} van: #{address}
                    ul.list-group
                        li.list-group-item
                            Strong Saldo:
                            |   #{balance}
                        li.list-group-item
                            Strong Permissies:
                            each val in permissions
                                |   #{val.type};
include ./includes/footer.pug